{
  "name": "omanyd",
  "version": "0.4.3",
  "contributors": [
    "Tom Andrews <git@tomandrews.co.uk>"
  ],
  "description": "DynamoDB data mapper for nodejs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "typecheck": "tsc --noEmit",
    "test": "./scripts/test.sh",
    "format": "prettier **/*.ts",
    "release": "np --no-publish --branch=main"
  },
  "repository": "git://github.com/tgandrews/omanyd.git",
  "keywords": [
    "omanyd",
    "datamapper",
    "DynamoDB",
    "aws",
    "amazon",
    "nosql"
  ],
  "engines": {
    "node": ">=14"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1140.0",
    "joi": "^17.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/uuid": "^8.3.4",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "np": "^7.6.1",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "wait-on": "^6.0.1"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "prettier --write"
    ]
  }
}
